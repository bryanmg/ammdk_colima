<div id="<%= dom_id user %>">

  <p>
    <strong>Name:</strong>
    <%= user.name %>
  </p>

  <p>
    <strong>Belt:</strong>
    <%= BELTS_TEXT[BELTS.key(user.belt)] %>
  </p>

  <p>
    <strong>Ocupation:</strong>
    <%= user.student_information&.ocupation %>
  </p>

  <br/>
  <h2 class="subtitle">Data of the responsible legal</h2>

  <p>
    <strong>Tutor:</strong>
    <%= user.student_information&.tutor_name %>
  </p>

  <p>
    <strong>Call to:</strong>
    <%= user.student_information&.cellphone %>
  </p>

  <p>
    <strong>Social security number:</strong>
    <%= user.student_information&.health_insurance %>
  </p>

</div>
